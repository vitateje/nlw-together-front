model=\"user.admin\" />\n        <label>Senha</label>\n        <input type=\"text\" placeholder=\"Password\" v-model=\"user.password\" />\n\n        <button class=\"waves-effect waves-light btn-small\">\n          Salvar<i class=\"material-icons left\">save</i>\n        </button>\n      </form>\n\n      <table>\n        <thead>\n          <tr>\n            <th>NOME</th>\n            <th>EMAIL</th>\n            <th>ADMIN</th>\n          </tr>\n        </thead>\n\n        <tbody>\n          <tr v-for=\"user of users\" :key=\"user.id\">\n            <td>{{ user.name }}</td>\n            <td>{{ user.email }}</td>\n            <td>{{ user.admin }}</td>\n            <td>\n              <button\n                @click=\"editar(user)\"\n                class=\"waves-effect btn-small blue darken-1\"\n              >\n                <i class=\"material-icons\">create</i>\n              </button>\n              <button\n                @click=\"remover(user)\"\n                class=\"waves-effect btn-small red darken-1\"\n              >\n                <i class=\"material-icons\">delete_sweep</i>\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport User from \"./services/produtos\";\n\nexport default {\n  data() {\n    return {\n\n      user: {\n        name:\"\",\n        email:\"\",\n        admin:\"\",\n      },\n\n      users: [],\n\n      errors: [],\n    };\n  },\n  mounted() {\n    this.listar()\n  },\n\n  methods:{\n\n    listar(){\n      User.listar().then((resposta) => {\n      this.users = resposta.data;\n    });\n    },\n\n    salvar(){\n\n      if(!this.user.id) {\n\n        User.salvar(this.user).then(resposta => {\n        this.user = {}\n        this.users = resposta.data;\n        this.listar()\n      }).catch(e => {\n        this.errors = e.reponse.data.errors\n      })\n\n      }else {\n        User.atualizar(this.user).then(resposta => {\n        this.user = {}\n        this.users = resposta.data;\n        this.listar()\n      }).catch(e => {\n        this.errors = e.reponse.data.errors\n      })\n      }\n\n\n    },\n\n    editar(user){\n      this.user = user\n    },\n\n    remover(user){\n      if(confirm('Deseja excluir o produto?')){\n        User.apagar(user).then(resposta =>{\n        then.users = resposta.data;\n        this.listar();\n        this.errors = []\n      }).catch(e => {\n        this.errors = e.reponse.data.errors\n      })\n      }\n    }\n\n  }\n};\n</script>\n\n<style>\n</style>\n"]}]}